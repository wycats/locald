{
  "name": "locald-dashboard-e2e",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "test": "playwright test --reporter=list",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "codegen": "playwright codegen",
    "screenshots": "node scripts/capture-dashboard-screenshots.mjs",
    "screenshots:ui": "playwright test tests/docs-screenshots.spec.ts --ui",
    "screenshots:update": "playwright test tests/docs-screenshots.spec.ts --update-snapshots",
    "screenshots:sync": "node scripts/sync-doc-screenshots.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
